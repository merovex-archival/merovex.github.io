layouts = []
pages.each do |p|
 x = p.split('layout: ')[1]
 layouts << x
end
# puts layouts.uniq.inspect
layouts.uniq!

files = Dir.glob("_layouts/*")

# puts files.inspect
files.each do |f|
	puts "File: #{File.basename(f)}" unless layouts.include?(File.basename(f).gsub('.html',''))
end
