puts x.inspect

Dir.glob("_includes/**/*.html") { |file|  
	file
	unless x.include?(file.gsub('_includes/',''))
		# puts "File: " + File.basename(file) 
		n = '_old' + file
		FileUtils.mkdir_p(File.dirname(n))
		puts "Move: #{file} #{n}"
		FileUtils.mv(file, n)
	end
	# puts file.inspect
}
